    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.0.1",
    "standard-version": "^4.0.0",
    "systemjs": "^0.20.17",
    "tslib": "^1.7.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.4"
  }
